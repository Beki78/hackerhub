{
  "name": "HackerHub",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "hackerrank.png"
  },

  "content_scripts": [
    {
      "js": ["dist/content.js"],
      "matches": [
        "https://www.hackerrank.com/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}
